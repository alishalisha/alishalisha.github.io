<%
  weeks ||= locals[:weeks] || []
  puts weeks.inspect
%>
<ul class="menu">
  <li class="menu-title"><a href="#"><%= app_name %></a></li>
  <% weeks.each_with_index do |week_data, index| %>
    <li>
      <a href="#"><%= week_data.detect{|row| row.kind == "page_title"}.try(:title) %></a>
    </li>
  <% end %>
</ul>